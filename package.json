{
  "name": "ethlete",
  "version": "0.0.0",
  "license": "GPL-3.0",
  "scripts": {
    "commit": "pnpm dlx git-cz",
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "prepare": "husky install",
    "format": "nx format:write --base=origin/main --head=HEAD",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "nx:update": "nx migrate latest",
    "nx:migrate": "nx migrate --run-migrations",
    "release": "pnpm prepare:release && pnpm changeset publish",
    "prepare:release": "pnpm config set no-git-checks true && sed -e \"s|libs/|dist/libs/|\" pnpm-workspace.yaml > pnpm-new.yaml && mv pnpm-new.yaml pnpm-workspace.yaml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.2.0",
    "@angular/common": "~14.2.0",
    "@angular/compiler": "~14.2.0",
    "@angular/core": "~14.2.0",
    "@angular/forms": "~14.2.0",
    "@angular/platform-browser": "~14.2.0",
    "@angular/platform-browser-dynamic": "~14.2.0",
    "@angular/router": "~14.2.0",
    "@changesets/cli": "2.24.3",
    "@nrwl/angular": "14.6.5",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.0",
    "@angular-eslint/eslint-plugin": "~14.0.0",
    "@angular-eslint/eslint-plugin-template": "~14.0.0",
    "@angular-eslint/template-parser": "~14.0.0",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "~14.2.0",
    "@angular/language-service": "~14.2.0",
    "@changesets/changelog-github": "0.4.6",
    "@nrwl/cli": "14.6.5",
    "@nrwl/cypress": "14.6.5",
    "@nrwl/eslint-plugin-nx": "14.6.5",
    "@nrwl/jest": "14.6.5",
    "@nrwl/linter": "14.6.5",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "14.6.5",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.33.1",
    "@typescript-eslint/parser": "~5.33.1",
    "cypress": "^10.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "~12.2.0",
    "nx": "14.6.5",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.7.2"
  }
}
